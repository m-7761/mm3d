#include <winresrc.h>

#include "copyright.h" //MM3D_PRODUCT/COPYRIGHT

A ICON "mm3d.ico"

/*CVTRES : fatal error CVT1100: duplicate resource.  
//type:MANIFEST, name:1, language:0x0409
CREATEPROCESS_MANIFEST_RESOURCE_ID RT_MANIFEST "win_manifest.xml"
*/

#define VER_PRODUCTVERSION			 1,3,11,0
#define VER_PRODUCTVERSION_STR		"1.3.11\0"

#ifndef DEBUG
#define VER_DEBUG						 0
#else
#define VER_DEBUG						 VS_FF_DEBUG
#endif

VS_VERSION_INFO VERSIONINFO
FILEVERSION	  VER_PRODUCTVERSION
PRODUCTVERSION  VER_PRODUCTVERSION
FILEFLAGSMASK	VS_FFI_FILEFLAGSMASK
FILEFLAGS		 (VER_DEBUG)
FILEOS			 VOS__WINDOWS32
FILETYPE		  VFT_APP
FILESUBTYPE	  VFT2_UNKNOWN
BEGIN
	 BLOCK "StringFileInfo"
	 BEGIN
		  BLOCK "040904E4"
		  BEGIN
				//VALUE "ProductName",	  "Maverick Model 3D"
				VALUE "ProductName",	  MM3D_PRODUCT
				VALUE "ProductVersion",  VER_PRODUCTVERSION_STR
				VALUE "FileVersion",	  VER_PRODUCTVERSION_STR
#ifdef _WIN64
				//VALUE "FileDescription", "Maverick Model 3D (x86_64)"
				VALUE "FileDescription",	MM3D_PRODUCT " (x86_64)"
				VALUE "OriginalFilename","mm3d.x86_64.exe"
				VALUE "InternalName",	 "mm3d.x86_64"
#else
				//VALUE "FileDescription", "Maverick Model 3D (x86)"
				VALUE "FileDescription",	MM3D_PRODUCT " (x86)"
				VALUE "OriginalFilename","mm3d.x86.exe"
				VALUE "InternalName",	 "mm3d.x86"
#endif
				// Copyright symbol (c) in Windows codepage 1252 is \xA9
				//VALUE "LegalCopyright",  "Copyright \xA9 2004-2008 Kevin Worcester,Copyright \xA9 2009-2019 Zack Middleton."
				VALUE "LegalCopyright",  MM3D_COPYRIGHT
		  END
	 END

	 BLOCK "VarFileInfo"
	 BEGIN
		  /* English language (0x409)in the Windows ANSI codepage (1252). */
		  VALUE "Translation",0x409,1252
	 END
END
